package  {		// importar no library path o ficheiro libs/FLVPlaybackAS3.swc	// calocar pasta "video" com video Promo.mp4" para dentro da pasta src	// importar o ficheiro 'FLVPlaybackAS3.swc' em 'Advanced ActionScript 3.0 Settings' na tab 'Library path'	import flash.display.Sprite;	import flash.display.Stage;	import fl.video.FLVPlayback;	import flash.events.Event;	import flash.events.KeyboardEvent;		public class video extends Sprite{				public var myVideo:FLVPlayback;		public var user:Boolean;		public var marker:Number;		public var marker1:Number;		public var markerRange:Number;		public var markerGap:Number;		public var cuePt:Object;		public var cuePt1:Object;		public var cuePt2:Object;		public var cuePt3:Object;		public var cuePt4:Object;		public var cuePt5:Object;		public var cuePt6:Object;		public var cuePt7:Object;		public var inc:Number;				public function video()		{			myVideo = new FLVPlayback();			myVideo.source = "/src/video/Promo.mp4";			myVideo.height = 800;			myVideo.width = 1280;			myVideo.scaleMode  = "exactFit";			addChild(myVideo);			//myVideo.playheadTime = 39;			user = false;						addEventListener(KeyboardEvent.KEY_UP,userBool);			myVideo.addEventListener(Event.ENTER_FRAME, fl_ClickToSeekToCuePoint);								}		public function fl_ClickToSeekToCuePoint(event:Event):void		{			if (!user)			{				if(myVideo.playheadTime > 6 && myVideo.playheadTime < 6.1)				{					myVideo.playheadTime = 2.4;				}				if(myVideo.playheadTime > 17.5 && myVideo.playheadTime < 17.6)				{					myVideo.playheadTime = 14.5;				}				if(myVideo.playheadTime > 23 && myVideo.playheadTime < 23.1)				{					myVideo.playheadTime = 21.6;				}				if(myVideo.playheadTime > 33 && myVideo.playheadTime < 33.1)				{					myVideo.playheadTime = 31.5;				}			}			if(myVideo.playheadTime > 41.5 && myVideo.playheadTime < 41.6)				{					myVideo.playheadTime = 0;				}		}		public function userBool(e:Event):void		{			trace(user);			if (user == true)			{				user = false;			} else {				user = true;			}		}	}}