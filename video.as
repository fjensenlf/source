package  {		// importar no library path o ficheiro libs/FLVPlaybackAS3.swc	// calocar pasta "video" com video Promo.mp4" para dentro da pasta src	// importar o ficheiro 'FLVPlaybackAS3.swc' em 'Advanced ActionScript 3.0 Settings' na tab 'Library path'	import flash.display.Sprite;	import fl.video.FLVPlayback;	import flash.events.Event;		public class video extends Sprite{				public var myVideo:FLVPlayback;		public var userV:uint;				public function video(playhead:Number)		{			userV = 0;			myVideo = new FLVPlayback();			myVideo.source = "/src/video/Promo.mp4";			myVideo.height = 800;			myVideo.width = 1280;			myVideo.scaleMode  = "exactFit";			myVideo.volume = 0;			addChild(myVideo);						myVideo.playheadTime = playhead;			myVideo.addEventListener(Event.ENTER_FRAME, fl_ClickToSeekToCuePoint);			addEventListener(CustomEvent.VIDEO_STATE, playLoop);								}		public function fl_ClickToSeekToCuePoint(event:Event):void		{			if (userV == 0)			{				if(myVideo.playheadTime > 5 && myVideo.playheadTime < 5.1)				{					myVideo.playheadTime = 2.4;				}				if(myVideo.playheadTime > 17.5 && myVideo.playheadTime < 17.6)				{					myVideo.playheadTime = 14.5;				}				if(myVideo.playheadTime > 23 && myVideo.playheadTime < 23.1)				{					myVideo.playheadTime = 21.6;				}				if(myVideo.playheadTime > 33 && myVideo.playheadTime < 33.1)				{					myVideo.playheadTime = 31.5;				}			}			if(myVideo.playheadTime > 41.5 && myVideo.playheadTime < 41.7)			{				myVideo.playheadTime = 0;			}		}		public function playLoop(event:CustomEvent):void		{			userV = event.io;		}		public function removeListeners():void{			removeEventListener(CustomEvent.VIDEO_STATE, playLoop);			myVideo.removeEventListener(Event.ENTER_FRAME, fl_ClickToSeekToCuePoint);		}	}}