package {    import flash.events.Event;    public class CustomEvent extends Event    {        public static const INSTANCE_NAME="instace name";		public static const GENERO="genero";		public static const DISPOSE="dispose";		public static const VIDEO_STATE="video state";		public var data:String;		public var io:uint;        public function CustomEvent(type:String, data, io)        {            super(type, data, io);			this.data = data;			this.io = io;        }    }}