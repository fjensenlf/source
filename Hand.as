package  {		import flash.display.Loader;	import flash.display.Bitmap;	import flash.display.Sprite;	import flash.net.URLRequest;	import caurina.transitions.Tweener;	import flash.events.Event;		public class Hand extends Sprite{		public var loader:Loader;		public var hand:Bitmap;		public function Hand() {						loader= new Loader();			loader.load(new URLRequest("/src/images/embedded/hand.png"));			loader.contentLoaderInfo.addEventListener(Event.COMPLETE, onComplete);								}		function onComplete(e:Event):void		{			hand = Bitmap(loader.content);			hand.x = 740;			hand.y = 300;			hand.rotation = 20;			          	hand.smoothing=true;          	addChild(hand);			tween();					}		public function tween() {			var setBool = function(bool:Boolean) {				if(bool){					remChild();				}			}			Tweener.addTween(hand, {x:470, rotation:10, time: 1, transition: "easeInQuad"});			Tweener.addTween(hand, {x:200, rotation:0, time: 1, transition: "easeOutQuad", delay: 1});			Tweener.addTween(hand, {x:470, rotation:10, time: 1, transition: "easeInQuad", delay: 2});			Tweener.addTween(hand, {x:740, rotation:20, time: 1, transition: "easeOutQuad", delay: 3});			Tweener.addTween(hand, {x:470, rotation:10, time: 1, transition: "easeInQuad", delay: 4});			Tweener.addTween(hand, {x:200, rotation:0, time: 1, transition: "easeOutQuad", delay: 5});			Tweener.addTween(hand, {x:470, rotation:5, time: 1, transition: "easeInQuad", delay: 6});			Tweener.addTween(hand, {x:740, rotation:10, time: 1, transition: "easeOutQuad", delay: 7});			Tweener.addTween(hand, {x:500, y:1100, rotation:0, time: 2.5, transition: "easeInOutCubic", delay: 8, onComplete:setBool, onCompleteParams:[true]});					}		public function remChild() {			trace("HAND REMOVED HAND");			dispatchEvent(new NavigationEvent(NavigationEvent.REMOVE_HAND));					}	}	}